{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Module",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "module": "iotedgedev.cli",
            "args": [
                "azure",
                "--update-dotenv"
            ],
            "debugOptions": [
                "RedirectOutput"
            ],
            "env": {
                "DOTENV_FILE": ".env.tmp",
            }
        }
    ]
}
        